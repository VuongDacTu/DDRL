@model IEnumerable<PJ_DGRL.Models.DGRLModels.GroupQuestion>
@{
    ViewData["Title"] = "kết quả đánh giá";
    Layout = "~/Areas/Student/Views/Shared/_LayoutStudent.cshtml";
}

<h4>Đáp án bạn đã trả lời</h4>
<hr />
<div class="row">
    <div class="col">
            @foreach (var questionGroup in Model)
            {
                <h4>@questionGroup.Name</h4>
                foreach (var question in questionGroup.QuestionLists)
                {
                    <h6>@question.ContentQuestion</h6>
                    foreach (var answer in question.AnswerLists)
                    {
     
                        if (answer.Checked == 1)
                        {
                        <p class="form-check-label" for="AnswerId[@answer.Id]">@answer.ContentAnswer (@answer.AnswerScore điểm)</p>
                        <br />
                        }
                    }
                }
            }
    </div>
    <div class="col">
        <h2>Tổng điểm tự đánh giá: @ViewBag.SumSelfPoint</h2>
    </div>
</div>



